<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  
 <servlet>
 		<servlet-name>hashir</servlet-name>
		<servlet-class>com.hashir.test.LoginServlet1</servlet-class>
 	</servlet>
 	<servlet>
 		<servlet-name>logout</servlet-name>
		<servlet-class>com.hashir.test.LogoutServlet</servlet-class>
 	</servlet>
 	<servlet>
 		<servlet-name>AddUser</servlet-name>
 		<servlet-class>com.hashir.test.AddUser</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>hashir</servlet-name>
  		<url-pattern>/authCheck1</url-pattern>
 	</servlet-mapping>
 	<servlet-mapping>
 		<servlet-name>logout</servlet-name>
  		<url-pattern>/doLogout</url-pattern>
 	</servlet-mapping>
 	<servlet-mapping>
 		<servlet-name>AddUser</servlet-name>
 		<url-pattern>/addUser</url-pattern>
 	</servlet-mapping>
</web-app>
